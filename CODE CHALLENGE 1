function Car(cname, speed) {
	this.cname = cname;
	this.speed = speed;
	this.accelerate = function() {
	this.speed += 10;
	};
	this.brake = function() {
	this.speed -= 5;
	};
  };

let honda = new Car('HONDA', 120);
let mercedes = new Car('Mercedes', 95);
console.log(honda);
honda.accelerate();
console.log(honda);
honda.brake();
console.log(honda);

console.log(mercedes);
mercedes.accelerate();
console.log(mercedes);
mercedes.brake();
console.log(mercedes);
